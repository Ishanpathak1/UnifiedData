      "public": "public",
      "rewrites": [
        {
          "source": "/api/**",
          "run": {
            "serviceId": "unifieddata-api",
            "region": "us-central1"
          }
        },
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  }
  