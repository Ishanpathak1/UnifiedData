{
    "hosting": [
      {
        "target": "unifieddata-api",
        "appHosting": {
          "run": {
            "runtime": "custom",
            "entry": "Dockerfile",
            "region": "us-central1"
          }
        }
      },
      {
        "target": "frontend",
        "public": "public",
       "rewrites": [
          {
            "source": "/api/**",
            "destination": "https://unifieddata-api-552541459765.us-central1.run.app/api/:path*"
          },
          {
            "source": "**",
            "destination": "/index.html"
          }
        ]
      }
    ]
  }
  
  